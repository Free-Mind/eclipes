public class Test {
	public void bubble(int[] arr,int length){
		int temp;
		int n=length;
		for(int j=0;j<length;j++){
			for(int i=0;i<n-1;i++){
				if(arr[i]>=arr[i+1]){
					temp=arr[i];
					arr[i]=arr[i+1];
					arr[i+1]=temp;
				}
				i++;
			}
			n--;
		}
		
		
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	}

}
